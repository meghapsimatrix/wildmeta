	file = {Cameron et al (2008) Wild bootstrap.pdf:/Users/meghajoshi/Box Sync/Dissertation_Joshi/articles/wild_bootstrapping/Cameron et al (2008) Wild bootstrap.pdf:application/pdf}
}

